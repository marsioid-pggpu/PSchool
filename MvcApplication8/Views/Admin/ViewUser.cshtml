@model IEnumerable<SchoolDomains.User>

@{
    ViewBag.Title = "Список пользователей";
}

<h2>Список пользователей</h2>

<p>
    @Html.ActionLink("Добавить пользователя", "AddUser")
</p>
<table>
    <tr>
        <th>
            Пользователи
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
            @Html.DisplayFor(modelItem => item.RegDate)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "EditUser", new { id=item.Id }) |
            @Html.ActionLink("Удалить", "DeleteUser", new { id=item.Id })
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Назад к администрированию", "Index", "Home")
</div>